{
	"name": "heropage",
	"version": "1.0.0",
	"description": "Portfolio",
	"main": "HeroPage.js",
	"scripts": {
		"start": "nodemon HeroPage.js",
		"nodestart": "node HeroPage.js",
		"sass": "node-sass -w scss/ -o dist/ --recursive",
